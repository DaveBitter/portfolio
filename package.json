    "@storybook/addon-links": "^5.3.21",
    "@storybook/addons": "^5.3.21",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/react": "^6.2.9",
    "@svgr/webpack": "^5.5.0",
    "@types/d3": "^5.16.4",
    "@types/enzyme": "^3.10.8",
    "@types/highlight.js": "^9.12.4",
