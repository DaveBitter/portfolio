    "@types/react-ga": "^2.3.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "@zeit/next-sass": "^1.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
